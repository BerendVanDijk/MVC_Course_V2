@model MVC_Course.ViewModels.MovieListViewModel
@{
    ViewBag.Title = "List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>

<table class="table_custom">
    <tr>
        <td>Movies</td>
    </tr>
    @foreach (var movie in Model.Movies)
    {
        <tr>
            <th>@movie.Name</th>
        </tr>

    }
    </table>
